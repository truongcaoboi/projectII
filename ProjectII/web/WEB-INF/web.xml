<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>GetDataFirst</servlet-name>
        <servlet-class>control.GetDataFirst</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Search</servlet-name>
        <servlet-class>control.Search</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>GetDetailJob</servlet-name>
        <servlet-class>control.GetDetailJob</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AutoLoad</servlet-name>
        <servlet-class>control.AutoLoad</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AutoLoadCompany</servlet-name>
        <servlet-class>control.AutoLoadCompany</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>GetDataFirst</servlet-name>
        <url-pattern>/GetDataFirst</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Search</servlet-name>
        <url-pattern>/Search</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>GetDetailJob</servlet-name>
        <url-pattern>/GetDetailJob</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AutoLoad</servlet-name>
        <url-pattern>/AutoLoad</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AutoLoadCompany</servlet-name>
        <url-pattern>/AutoLoadCompany</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <welcome-file-list>
        <welcome-file>index.html</welcome-file>
    </welcome-file-list>
</web-app>
